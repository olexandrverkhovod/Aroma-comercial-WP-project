{
	"parser": "babel-eslint",
	"parserOptions": {
		"sourceType": "module",
		"allowImportExportEverywhere": true
	},
	"env": {
	    "es6": true,
	    "browser": true
	},
	"rules": {
		"max-len": [1, 120, 2, 
			{"ignoreComments": true}
		],
		"prop-types": [1],
		"no-tabs":[0],
		"no-undef":[1],
		"no-unused-vars":[1],
		"no-useless-constructor":[1],
		"class-methods-use-this":[1],
		"indent":[0],
		"no-console": [0]
//		"no-console": [
//			"error",
//			{
//				"allow": ["warn", "error"]
//			}
//		]
	}
}